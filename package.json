{
  "name": "@deskevinmendez/vue-k-alendar",
  "version": "0.0.2",
  "description": "A simple calendar component for Vue 3",
  "license": "Apache-2.0",
  "author": {
    "name": "Kevin MÃ©ndez",
    "email": "kevinmendez0530@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesKevinMendez/vue-k-alendar"
  },
  "bugs": {
    "url": "https://github.com/DesKevinMendez/vue-k-alendar/issues"
  },
  "keywords": [
    "calendar",
    "vue 3"
  ],
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build": "pnpm clean && run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/k-alendar.umd.cjs",
  "module": "./dist/k-alendar.js",
  "exports": {
    ".": {
      "import": "./dist/k-alendar.js",
      "require": "./dist/k-alendar.umd.cjs",
      "types": "./dist/types/src/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "dependencies": {
    "luxon": "^3.5.0"
  },
  "devDependencies": {
    "uuid": "^10.0.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "@vuepress/bundler-vite": "2.0.0-rc.14",
    "@vuepress/theme-default": "2.0.0-rc.40",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "git-cz": "^4.9.0",
    "jsdom": "^24.1.0",
    "mockdate": "^3.0.5",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.41",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.7",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.1.0",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.36",
    "vue-tsc": "^2.0.21",
    "vuepress": "2.0.0-rc.14"
  }
}
